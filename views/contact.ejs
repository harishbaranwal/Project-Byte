              <img src="../images/a10.jpg">
            </div>
          </div>
          <!-- Start Image Slider -->
        </ul>
        <!-- End Image Section-->
      </div>
      <!-- End Footer Body -->
    </div>


    <!-- Start Footer bottom Part -->
    <div class="footer-txt">
      <div class="footer-txt-content">
        <!-- Start Footer bottom  Left Part -->
        <div class="policy_terms">
          <h4>Privacy Policy</h4>
          <h4>Terms & Conditions</h4>
